pipeline {
    agent any
    stages{
		stage('pullLatestCode'){
            steps {
			git branch: 'master',
			 url: 'https://github.com/APdrone/lighthouse-jenkin-integration'
            }
        }
        stage("Run tests") {
            steps {
                bat 'npm install'
		bat 'npx lighthouse-ci http://localhost --jsonReport --report=.'lighthouseReport('./report.json')
            }

        }
    }
}
